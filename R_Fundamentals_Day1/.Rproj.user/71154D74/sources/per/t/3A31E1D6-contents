## ------------------------------------------------------------------------
# AND(&) - con1(T) & con2 (T) = TRUE #++=+,+-=-,--=-,-+=-
# OR(|) - con1(T) | con2 (F) = TRUE#++=+,+-=+,-+=+,--=-
x=10
x>=11 & x<=1
z=(x>=1)&(x<=19)
z
